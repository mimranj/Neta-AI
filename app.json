{
  "expo": {
    "name": "neta-ai",
    "slug": "neta-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "scheme": "neta-ai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.mimranj.netaai",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "requireFullScreen": true,
      "usesIcloudStorage": false,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to upload images.",
        "NSFilesystemUsageDescription": "This app requires access to your file system to upload files.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification",
          "processing"
        ]
      },
      "privacyPolicyUrl": "https://askneta.com/privacy-and-policy/vacy",
      "termsOfServiceUrl": "https://askneta.com/privacy-and-policy/terms"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mimranj.netaai",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "apple.com.mimranj.netaai",
          "enableGooglePay": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "051b1846-ac48-4c82-8b68-a7c5c6fa325c"
      }
    }
  }
}